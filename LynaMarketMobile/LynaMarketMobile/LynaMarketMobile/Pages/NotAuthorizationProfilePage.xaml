<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LynaMarketMobile.Pages.NotAuthorizationProfilePage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="End">
            <Button x:Name="AuthorizationButton" 
                    Text="Авторизация"
                    Clicked="AuthorizationButtonOnClicked"/>
            <Button x:Name="RegistrationButton" 
                    Text="Регистрация"
                    Clicked="RegistrationButtonOnClicked"/>
            <Button x:Name="AboutButton" 
                    Text="О программе"
                    Clicked="AboutButtonOnClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>